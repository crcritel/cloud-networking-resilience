        <mxCell id="Dv-mLlJBzES2whVv2Aaz-17" value="DNS Policy withdraws impaired Region&lt;div&gt;GA reroute live traffic to healthy Region in ms&lt;/div&gt;" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=#9673a6;fillColor=#e1d5e7;sketch=1;curveFitting=1;jiggle=2;fontStyle=1;fontSize=15;" parent="1" vertex="1">
